<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id = "engine" class = "com.caps.fs.Springonescb.Engine">
		<property name = "brand" value = "VW"></property>
		<property name = "manufacturingUnit" value = "Germany"></property>
	</bean>
	<bean id = "car" class = "com.caps.fs.Springonescb.Car">
		<property name = "vehicleNo" value = "8402"></property>
		<property name = "name" value = "Taigun"></property>
		<property name = "litre" value = "1.0"></property>
		<property name = "engine" ref = "engine"></property>
	</bean>
	
	<bean id = "add1" class = "com.caps.fs.Springonescb.Address">
		<property name = "id" value = "1"></property>
		<property name = "address1" value = "LA"></property>
	</bean>

	<bean id = "add2" class = "com.caps.fs.Springonescb.Address">
		<property name = "id" value = "1"></property>
		<property name = "address1" value = "Bali"></property>
	</bean>
	
	
	<bean id = "pr" class = "com.caps.fs.Springonescb.Person">
		<property name = "id" value = "1"></property>
		<property name = "name" value = "Alexa"></property>
		<property name = "characterstics">
			<list> 
				<value>Alexa is a good person.</value>
				<value>Alexa is Smart.</value>
				<value>Alexa is active person</value>
			 </list>
		</property>
		<property name="address">
			<list>
				<ref bean="add1" />
				<ref bean="add2"/>
			
			</list>
		
		</property>
	</bean>
</beans>
